{
  "name": "appic-cli",
  "version": "1.0.1",
  "description": "Command line interface for rapid apply development",
  "main": "lib/appic.js",
  "scripts": {
    "commit": "git add . && git cz",
    "build": "rm -rf ./lib/* && ./node_modules/.bin/tsc",
    "watch": "./node_modules/.bin/tsc -w",
    "build:com": "node ./lib/bin/appic.js",
    "prepublish": "npm run build"
  },
  "bin": {
    "appic": "lib/bin/appic.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djkloop666/appic-cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/djkloop666/appic-cli/issues"
  },
  "homepage": "https://github.com/djkloop666/appic-cli#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/colors": "^1.2.1",
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^5.0.4",
    "@types/inquirer": "0.0.43",
    "@types/node": "^10.9.4",
    "@types/semver": "^5.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "colors": "^1.3.2",
    "commander": "^2.18.0",
    "download-git-repo": "^1.1.0",
    "fs-extra": "^7.0.0",
    "git-down-repo": "^2.4.2",
    "inquirer": "^6.2.0",
    "ora": "^3.0.0",
    "semver": "^5.5.1",
    "validate-npm-package-name": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9"
  }
}
